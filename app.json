        "foregroundImage": "./assets/adaptive-icon1.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.dhanieldcoder.igoepp_supplier"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "2fead74d-7e89-48f9-9cc2-7c3bcaabb533"
      }
    }
  }
